{
  "name": "domain-proxy-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker pour router les domaines personnalis√©s",
  "main": "worker.js",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create DOMAINS_KV",
    "kv:list": "wrangler kv:key list --namespace-id=$KV_NAMESPACE_ID --prefix=domain:",
    "kv:add": "wrangler kv:key put --namespace-id=$KV_NAMESPACE_ID",
    "kv:delete": "wrangler kv:key delete --namespace-id=$KV_NAMESPACE_ID"
  },
  "keywords": ["cloudflare", "workers", "domain", "proxy"],
  "author": "Built by Magellan",
  "license": "MIT"
}
