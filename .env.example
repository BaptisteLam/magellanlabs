# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-publishable-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# VibeSDK Configuration (Cloudflare)
# Enable VibeSDK integration
VITE_USE_VIBESDK="true"

# Your VibeSDK API key from https://build.cloudflare.dev
VITE_VIBESDK_API_KEY="your-vibesdk-api-key"

# VibeSDK base URL (optional, defaults to https://build.cloudflare.dev)
VITE_VIBESDK_BASE_URL="https://build.cloudflare.dev"
